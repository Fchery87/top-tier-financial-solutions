{
  "openapi": "3.0.3",
  "info": {
    "title": "Top Tier Financial Solutions API",
    "version": "1.0.0",
    "description": "API for Top Tier Financial Solutions website, supporting public content display and lead generation, and an authenticated admin interface for content management."
  },
  "servers": [
    {
      "url": "/api/v1",
      "description": "API v1"
    }
  ],
  "tags": [
    {
      "name": "public",
      "description": "Publicly accessible endpoints for website content and lead generation."
    },
    {
      "name": "auth",
      "description": "Authentication and authorization for admin users."
    },
    {
      "name": "admin-content",
      "description": "Admin endpoints for managing website content (pages, testimonials, FAQs, disclaimers)."
    },
    {
      "name": "admin-leads",
      "description": "Admin endpoints for managing contact form submissions and consultation bookings."
    }
  ],
  "paths": {
    "/public/content/{slug}": {
      "get": {
        "tags": ["public"],
        "summary": "Retrieve specific website content by slug",
        "operationId": "getWebsiteContentBySlug",
        "parameters": [
          {
            "name": "slug",
            "in": "path",
            "required": true,
            "description": "Slug of the content item (e.g., 'home', 'services', 'privacy-policy')",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Content item retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WebsiteContent"
                },
                "examples": {
                  "homepage": {
                    "value": {
                      "id": "a1b2c3d4-e5f6-7890-1234-567890abcdef",
                      "slug": "home",
                      "title": "Welcome to Top Tier Financial Solutions",
                      "content": "<p>Your trusted partner for credit repair in New York...</p>",
                      "content_type": "page",
                      "meta_title": "Top Tier Financial Solutions - Credit Repair NY",
                      "meta_description": "Expert credit repair services in New York. Understand your rights and improve your credit score.",
                      "is_published": true,
                      "created_at": "2025-11-29T10:00:00Z",
                      "updated_at": "2025-11-29T10:00:00Z"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
